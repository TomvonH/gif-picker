 @click.native=\"$router.push('favorites')\"\n        >{{checkFavorites}}</BaseButton>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport BaseButton from \"./BaseButton\";\nexport default {\n  components: {\n    BaseButton\n  },\n  props: {\n    title: String,\n    placeholder: String\n  },\n  computed: {\n    allGifs() {\n      return this.$store.getters.allGifs;\n    }\n  },\n  data() {\n    return {\n      searchTerm: \"\",\n      search: \"Search\",\n      checkFavorites: \"View your favorites\"\n    };\n  },\n  methods: {\n    getGifs() {\n      this.$emit(\"getGifs\", this.searchTerm);\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n@import \"@/scss/_variables\";\n\n.input {\n  background-color: transparent;\n  color: $card-bg;\n  width: auto;\n  margin: 0;\n  border-top-right-radius: 0;\n  border-bottom-right-radius: 0;\n  flex-grow: 1;\n  &:focus {\n    border-color: $card-bg;\n  }\n  &::placeholder {\n    color: lighten($card-bg, 20%);\n  }\n}\n.search-field {\n  display: flex;\n  flex-basis: 75%;\n\n  h2 {\n    font-size: 32px;\n  }\n  input,\n  button {\n    font-size: 32px;\n    border-width: 4px;\n  }\n\n  button:first-of-type {\n    border-left-width: 0px;\n    border-top-left-radius: 0px;\n    border-bottom-left-radius: 0px;\n  }\n  button:last-of-type {\n    margin-left: 10px;\n  }\n}\n.hero-title {\n  font-size: 48px;\n}\n.hero-bar {\n  height: 260px;\n}\n.hero-bar-scaled {\n  height: 130px;\n  transition: all 0.2s ease-in;\n  padding-bottom: 0;\n}\n\n.pb-lg {\n  padding-bottom: 100px;\n}\n\n.scaler {\n  transform: translateY(-15px) scale(0.5, 0.5);\n  transition: all 0.2s ease-in;\n}\n\n.search-field-inline {\n  flex-basis: 0;\n  margin: auto;\n  margin-left: 10px;\n  color: $card-bg;\n  font-weight: 300;\n}\n.hero-container {\n  padding: 0;\n}\n</style>\n"]}]}