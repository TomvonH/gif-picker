ete.vue\";\nexport default {\n  components: {\n    BaseButton,\n    DeleteIcon\n  },\n  props: {\n    gif: Object,\n    isFavorite: Boolean\n  },\n\n  computed: {\n    setSrc() {\n      return `https://media.giphy.com/media/${this.gif.id}/200.gif`;\n    },\n    setComment() {\n      if (this.gif.comments.length === 1) {\n        return \"Comment\"\n      }\n      return \"Comments\"\n    }\n  },\n\n  methods: {\n    removeFromFavorites() {\n      this.$emit(\"removeFromFavorites\");\n    },\n    openGifInModal() {\n      this.$emit(\"openGifInModal\");\n    }\n  }\n};\n</script>\n\n\n"]}]}