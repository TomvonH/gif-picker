earch-field-lg': allGifs.length <= 0 }\">\n      <input\n        v-model=\"searchTerm\"\n        @keyup.enter=\"getGifs\"\n        type=\"text\"\n        :placeholder=\"placeholder\"\n        autofocus\n        class=\"input\"\n      />\n      <BaseButton class=\"btn-inverted\" @click.native=\"getGifs\">{{search}}</BaseButton>\n      <h2 class=\"search-field-inline\">or</h2>\n      <BaseButton class=\"btn-inverted\" @click.native=\"$router.push('favorites')\">{{checkFavorites}}</BaseButton>\n    </div>\n  </div>\n</template>\n\n<script>\nimport BaseButton from \"./BaseButton\";\nexport default {\n  components: {\n    BaseButton\n  },\n  props: {\n    title: String,\n    placeholder: String\n  },\n  computed: {\n    allGifs() {\n      return this.$store.getters.allGifs;\n    }\n  },\n  data() {\n    return {\n      searchTerm: \"\",\n      search: \"Search\",\n      checkFavorites: \"Check my favorites\"\n    };\n  },\n  methods: {\n    getGifs() {\n      this.$emit(\"getGifs\", this.searchTerm);\n    }\n  }\n};\n</script>\n<style lang=\"scss\" scoped>\n@import \"@/scss/_variables\";\n\n.input {\n  background-color: transparent;\n  color: $card-bg;\n  width: auto;\n  margin: 0;\n  border-top-right-radius: 0;\n  border-bottom-right-radius: 0;\n  flex-grow: 1;\n  &:focus {\n    border-color: $card-bg;\n  }\n  &::placeholder {\n    color: lighten($card-bg, 20%);\n  }\n}\n.search-field {\n  display: flex;\n  flex-basis: 50%;\n  transition: all 0.2s ease-in;\n\n  button:first-of-type {\n    border-left-width: 0px;\n    border-top-left-radius: 0px;\n    border-bottom-left-radius: 0px;\n  }\n  button:last-of-type {\n    margin-left: 10px;\n  }\n}\n.hero-bar,\n.hero-title {\n  transition: all 0.2s ease-in;\n}\n.hero-title-lg {\n  padding-bottom: 24px;\n  transform: translateZ(0) scale(1.5, 1.5);\n  backface-visibility: hidden;\n}\n.pb-lg {\n  padding-bottom: 100px;\n}\n\n.search-field-lg {\n  transform: translateZ(0) scale(1.5, 1.5);\n  backface-visibility: hidden;\n}\n\n.se